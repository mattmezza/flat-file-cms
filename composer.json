{
    "name": "mattmezza/flat-file-cms",
    "description": "Need a CMS module for your website? This one's based on flat files",
    "type": "library",
    "require": {
      "php": ">=5.6.0",
      "suin/php-rss-writer": "^1.5",
      "erusev/parsedown-extra": "^0.7.1",
      "symfony/yaml": "^3.1"
    },
    "require-dev": {
      "phpunit/phpunit": "5.5.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matteo Merola",
            "email": "mattmezza@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "FlatFileCMS\\": "src/",
            "FlatFileCMS\\Content\\": "src/content",
            "FlatFileCMS\\Plugins\\": "src/plugins",
            "FlatFileCMS\\Users\\": "src/users",
            "FlatFileCMS\\Exceptions\\": "src/exceptions"
        }
    }
}
